{
  "name": "youway-mobile-app",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:ios": "eas build --platform ios --profile production",
    "submit:ios": "eas submit --platform ios --latest",
    "rebuild:ios": "rm -rf ios && npx expo prebuild --platform ios --clean && cd ios && rm -rf Pods Podfile.lock && pod cache clean --all && pod install --repo-update",
    "rebuild:android": "rm -rf android &&  npx expo prebuild --platform android --clean && cd android && ./gradlew clean && ./gradlew build && eas build --platform android --profile production",
    "build:android": "eas build -p android --profile production --clear-cache",
    "submit:android": "eas submit --platform android --latest"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@legendapp/motion": "^2.5.3",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/native": "^7.1.6",
    "@types/react": "~19.1.10",
    "babel-plugin-module-resolver": "^5.0.2",
    "expo": "~54.0.32",
    "expo-font": "~14.0.8",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.4",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-system-ui": "~6.0.7",
    "expo-web-browser": "~15.0.7",
    "lucide-react-native": "^0.510.0",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-worklets": "^0.7.2",
    "tailwindcss": "^3.4.19"
  },
  "devDependencies": {
    "@types/react": "~19.1.10",
    "babel-preset-expo": "~12.0.1",
    "typescript": "~5.9.2"
  },
  "private": true
}
